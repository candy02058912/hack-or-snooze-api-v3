{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-06/schema#",
  "type": "object",
  "id": "/",
  "additionalProperties": false,
  "properties": {
    "favorites": {
      "type": "array",
      "id": "/properties/favorites",
      "minItems": 1,
      "items": {
        "type": "string",
        "id": "/properties/favorites/items",
        "minLength": 1,
        "maxLength": 55
      }
    },
    "name": {
      "type": "string",
      "id": "/properties/name",
      "minLength": 1,
      "maxLength": 55
    },
    "password": {
      "type": "string",
      "id": "/properties/password",
      "minLength": 1,
      "maxLength": 55
    },
    "username": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9]*$",
      "id": "/properties/username",
      "minLength": 1,
      "maxLength": 55
    }
  },
  "required": ["name", "password", "username"]
}
