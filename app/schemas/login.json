{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-06/schema#",
  "type": "object",
  "id": "/",
  "additionalProperties": false,
  "properties": {
    "password": {
      "type": "string",
      "id": "/properties/password",
      "minLength": 1,
      "maxLength": 55
    },
    "username": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9]*$",
      "id": "/properties/username",
      "minLength": 1,
      "maxLength": 55
    }
  },
  "required": ["password", "username"]
}
